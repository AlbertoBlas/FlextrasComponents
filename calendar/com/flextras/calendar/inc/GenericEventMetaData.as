/**
 * This event is dispatched when a user presses and releases the main button of the user's pointing device over the list in this dayRenderer.  
 * This event mirrors MouseEvent.CLICK
 *
 *  @eventType com.flextras.calendar.CalendarMouseEvent.CLICK_DAY
 */
[Event(name="dayClick", type="com.flextras.calendar.CalendarMouseEvent")]

