/**
 *  This event is dispatched when the user releases clicks the button to get to the next week in the weekHeaderRenderer. 
 *
 *  @eventType com.flextras.calendar.CalendarChangeEvent.NEXT_WEEK
 */
[Event(name="nextWeek", type="com.flextras.calendar.CalendarChangeEvent")]

/**
 *  This event is dispatched when the user clicks the button to get to the previous week in the weekHeaderRenderer. 
 *
 *  @eventType com.flextras.calendar.CalendarChangeEvent.PREVIOUS_WEEK
 */
[Event(name="previousWeek", type="com.flextras.calendar.CalendarChangeEvent")]